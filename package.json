  "scripts": {
    "start": "craco start",
    "start:dev": "npm run update-icons && npm run lingui:compile && craco start",
    "build": "npm run lingui:compile && craco build",
    "lint": "eslint --max-warnings=0 ./src",
    "prettier": "prettier --check ./src/**/*.js",
    "format": "eslint --fix ./src && prettier --write ./src/**/*.js",
    "test": "cross-env CI=true craco test --env=jsdom",
    "test:watch": "craco test",
    "lingui:add-locale": "lingui add-locale",
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile",
    "lingui:clean": "lingui extract --clean",
    "update-icons": "node ./scripts/update-icons.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@lingui/cli": "^2.9.1",
    "@lingui/macro": "^2.9.1",
    "babel-core": "^7.0.0-bridge.0",
    "cross-env": "^6.0.3"
  }
